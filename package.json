{
  "name": "shikosai32-server",
  "version": "0.1.0",
  "private": true,
  "license": "Salty",
  "scripts": {
    "gen:module": "nest generate module",
    "gen:controller": "nest generate controller",
    "gen:service": "nest generate service",
    "prebuild": "rimraf dist",
    "build": "prisma generate --schema ./src/libs/prisma/schema.prisma && nest build",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "prod:dev": "cross-env NODE_ENV=development node dist/main",
    "lint": "eslint ./src/**/*.ts",
    "test": "cross-env NODE_ENV=test jest --silent=false --verbose false --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watch --passWithNoTests",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "test:e2e": "cross-env NODE_ENV=test jest --silent=false --verbose false --config ./test/jest-e2e.json --passWithNoTests",
    "prisma:gen:dev": "cross-env NODE_ENV=development prisma generate --schema ./src/libs/prisma/schema.prisma",
    "prisma:clear:dev": "cross-env NODE_ENV=development prisma db push --accept-data-loss --schema ./src/libs/prisma/schema.prisma",
    "prisma:studio:dev": "cross-env NODE_ENV=development prisma studio --schema ./src/libs/prisma/schema.prisma",
    "prisma:gen:prod": "cross-env NODE_ENV=production prisma generate --schema ./src/libs/prisma/schema.prisma",
    "prisma:clear:prod": "cross-env NODE_ENV=production prisma db push --accept-data-loss --schema ./src/libs/prisma/schema.prisma",
    "prisma:studio:prod": "cross-env NODE_ENV=production prisma studio --schema ./src/libs/prisma/schema.prisma",
    "prisma:gen:test": "cross-env NODE_ENV=test prisma generate --schema ./src/libs/prisma/schema.prisma",
    "prisma:clear:test": "cross-env NODE_ENV=test prisma db push --accept-data-loss --schema ./src/libs/prisma/schema.prisma",
    "prisma:studio:test": "cross-env NODE_ENV=test prisma studio --schema ./src/libs/prisma/schema.prisma",
    "apollo:rover": "rover graph publish shikosai32@current --schema ./schema.gql"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.22",
    "@nestjs/cli": "^9.1.1",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/graphql": "^10.0.22",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/testing": "^9.0.11",
    "@prisma/client": "^4.3.0",
    "apollo-server-core": "^3.10.2",
    "apollo-server-types": "^3.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "firebase": "^9.9.3",
    "firebase-admin": "^11.0.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "jest-mock-extended": "^2.0.7",
    "log4js": "^6.6.1",
    "rxjs": "^7.5.6",
    "supertest-graphql": "^1.1.4",
    "ts-pattern": "^4.0.5"
  },
  "devDependencies": {
    "@apollo/rover": "^0.8.1",
    "@nestjs/schematics": "^9.0.1",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "apollo-server-express": "^3.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.3.0",
    "prisma-nestjs-graphql": "^17.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
