{
  "name": "shikosai32-server",
  "version": "0.1.0",
  "private": true,
  "license": "Salty",
  "scripts": {
    "gen:module": "nest generate module",
    "gen:controller": "nest generate controller",
    "gen:service": "nest generate service",
    "prebuild": "rimraf dist",
    "build": "prisma generate --schema ./src/infra/prisma/schema.prisma && nest build",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "prod:dev": "cross-env NODE_ENV=development node dist/main",
    "lint": "eslint ./src/**/*.ts",
    "test": "cross-env NODE_ENV=test jest --silent=false --verbose false --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watch --passWithNoTests",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "test:e2e": "cross-env NODE_ENV=test jest --silent=false --verbose false --config ./test/jest-e2e.json --passWithNoTests",
    "prisma:gen:dev": "cross-env NODE_ENV=development prisma generate --schema ./src/infra/prisma/schema.prisma",
    "prisma:seed:dev": "cross-env NODE_ENV=development ts-node ./src/infra/prisma/seed/main.seed.ts",
    "prisma:clear:dev": "cross-env NODE_ENV=development prisma db push --accept-data-loss --schema ./src/infra/prisma/schema.prisma",
    "prisma:studio:dev": "cross-env NODE_ENV=development prisma studio --schema ./src/infra/prisma/schema.prisma",
    "prisma:gen:prod": "cross-env NODE_ENV=production prisma generate --schema ./src/infra/prisma/schema.prisma",
    "prisma:clear:prod": "cross-env NODE_ENV=production prisma db push --accept-data-loss --schema ./src/infra/prisma/schema.prisma",
    "prisma:studio:prod": "cross-env NODE_ENV=production prisma studio --schema ./src/infra/prisma/schema.prisma",
    "prisma:gen:test": "cross-env NODE_ENV=test prisma generate --schema ./src/infra/prisma/schema.prisma",
    "prisma:clear:test": "cross-env NODE_ENV=test prisma db push --accept-data-loss --schema ./src/infra/prisma/schema.prisma",
    "prisma:studio:test": "cross-env NODE_ENV=test prisma studio --schema ./src/infra/prisma/schema.prisma",
    "apollo:rover": "rover graph publish shikosai32@current --schema ./schema.gql"
  },
  "dependencies": {
    "@nestjs/apollo": "10.1.6",
    "@nestjs/cli": "9.1.5",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/graphql": "10.1.6",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/testing": "9.2.1",
    "@prisma/client": "4.7.1",
    "apollo-server-core": "3.10.3",
    "apollo-server-types": "3.7.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dataloader": "2.1.0",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "firebase": "9.14.0",
    "firebase-admin": "11.3.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-tag": "2.12.6",
    "graphql-validation-complexity": "0.4.2",
    "supertest-graphql": "1.1.4",
    "ts-pattern": "4.0.6",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@apollo/rover": "0.10.0",
    "@nestjs/schematics": "9.0.3",
    "@types/graphql-validation-complexity": "0.4.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.10",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "apollo-server-express": "3.11.1",
    "cross-env": "7.0.3",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "graphql": "16.6.0",
    "graphql-type-json": "0.3.2",
    "jest": "29.3.1",
    "prettier": "2.8.0",
    "prisma": "4.7.1",
    "prisma-nestjs-graphql": "17.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  }
}
